<table class="table-striped">
  <thead>Events for <%= @event.name %> </thead>
<br>
  <% @events_tasks.each do |task| %>
    <tr>
      <td><%= task.date %></td>
    <td><% t = Task.find(task.task_id) %> <%= t.name %></td>
    <td><%= Category.find(t.category_id).name %></td>
    <% end %>
    </tr>
</table>

<%= link_to "Edit these dates", edit_event_patch_path %>



